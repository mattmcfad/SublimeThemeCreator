.wrapper
	max-width $max-width
	margin 0 auto
	border 5px solid red
	border-radius 5px

	.half
		float: left
		width: 50%
		border 3px solid salmon
		border-radius 3px

	ol
		counter-reset item
		list-style-type none
		li
			display block
			&:before
				content counter(item) " "
				counter-increment item